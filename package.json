{
  "package_version": 2,
  "version": "1.0.0",
  "name": "tiny-inspector",
  "description": "i18n:tiny-inspector.description",
  "main": "./dist/main.js",
  "dependencies": {
    "fs-extra": "^10.0.0",
    "vue": "^3.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/fs-extra": "^9.0.5",
    "@types/node": "^16.0.1",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/devtools": "^6.0.12",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.23",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.8",
    "typescript": "^4.3.4",
    "vite": "^2.8.4"
  },
  "panels": {
    "default": {
      "title": "tiny-inspector Default Panel",
      "type": "dockable",
      "main": "dist/panels/default.js",
      "size": {
        "min-width": 400,
        "min-height": 300,
        "width": 600,
        "height": 600
      }
    },
    "counter": {
      "title": "tiny-inspector Counter Panel",
      "type": "dockable",
      "main": "dist/panels/counter.js",
      "size": {
        "min-width": 400,
        "min-height": 300,
        "width": 800,
        "height": 600
      }
    }
  },
  "contributions": {
    "menu": [
      {
        "path": "i18n:menu.panel/tiny-inspector",
        "label": "i18n:tiny-inspector.open_panel",
        "message": "open-panel"
      }
    ],
    "messages": {
      "open-panel": {
        "methods": [
          "openPanel"
        ]
      }
    },
    "scene": {
      "script": "dist/scene/index.js"
    }
  },
  "author": "Cocos Creator",
  "editor": ">=3.4.0",
  "scripts": {
    "build:panels:dev": "cross-env NODE_ENV=development node scripts/panels.js",
    "build:main:dev": "cross-env NODE_ENV=development node scripts/main.js",
    "build:scene:dev": "cross-env NODE_ENV=development babel src/scene/index.ts --out-file dist/scene/index.js"
  }
}
