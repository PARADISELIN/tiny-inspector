{
  "package_version": 2,
  "version": "1.0.0",
  "name": "tiny-inspector",
  "description": "i18n:tiny-inspector.description",
  "main": "./dist/main.js",
  "dependencies": {
    "fs-extra": "^10.0.0",
    "vue": "^3.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/fs-extra": "^9.0.5",
    "@types/node": "^16.0.1",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "panels": {
    "default": {
      "title": "tiny-inspector Default Panel",
      "type": "dockable",
      "main": "dist/panels/default/script",
      "size": {
        "min-width": 400,
        "min-height": 300,
        "width": 600,
        "height": 600
      }
    }
  },
  "contributions": {
    "menu": [
      {
        "path": "tiny-inspector",
        "label": "i18n:tiny-inspector.open_panel",
        "message": "open-panel"
      }
    ],
    "messages": {
      "open-panel": {
        "methods": [
          "openPanel"
        ]
      }
    },
    "scene": {
      "script": "dist/scene/index.js"
    }
  },
  "author": "Cocos Creator",
  "editor": ">=3.4.0",
  "scripts": {
    "build": "rimraf dist && babel src -d dist -x \".ts,.tsx\"",
    "watch": "rimraf dist && babel src -d dist -x \".ts,.tsx\" -w"
  }
}
